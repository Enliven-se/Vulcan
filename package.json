    "lintfix": "eslint --cache --fix --ext .jsx,js packages",
    "test-unit": "TEST_CHROME_ARGS='--remote-debugging-port=9222' TEST_BROWSER_DRIVER=chrome meteor test-packages ./packages/* --port 60859 --driver-package meteortesting:mocha --raw-logs",
    "test-client": "TEST_SERVER=0 TEST_CHROME_ARGS='--remote-debugging-port=9222' TEST_BROWSER_DRIVER=chrome meteor test-packages ./packages/* --port 60859 --driver-package meteortesting:mocha --raw-logs",
